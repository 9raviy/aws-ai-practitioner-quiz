{
  "name": "aws-ai-quiz-backend",
  "version": "1.0.0",
  "description": "AWS Bedrock-powered quiz backend with Claude 3.5 Sonnet integration",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon src/app.ts",
    "start": "node dist/app.js",
    "server": "npx ts-node src/app.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:bedrock": "npx ts-node src/test-bedrock.ts",
    "test:quick": "npx ts-node src/quick-test.ts",
    "test:lambda": "npx ts-node src/test-lambda-local.ts",
    "test:api": "npx ts-node src/test-phase2-api.ts",
    "deploy": "powershell -ExecutionPolicy Bypass -File deploy.ps1",
    "deploy:dev": "powershell -ExecutionPolicy Bypass -File deploy.ps1 dev us-west-2",
    "deploy:staging": "powershell -ExecutionPolicy Bypass -File deploy.ps1 staging us-west-2",
    "deploy:prod": "powershell -ExecutionPolicy Bypass -File deploy.ps1 prod us-west-2",
    "verify:aws": "powershell -ExecutionPolicy Bypass -File verify-aws-setup.ps1",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "aws",
    "bedrock",
    "claude",
    "lambda",
    "quiz",
    "ai-practitioner"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-bedrock": "^3.848.0",
    "@aws-sdk/client-bedrock-runtime": "^3.848.0",
    "@aws-sdk/client-dynamodb": "^3.848.0",
    "@aws-sdk/lib-dynamodb": "^3.850.0",
    "@types/uuid": "^10.0.0",
    "aws-lambda": "^1.0.7",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "express-rate-limit": "^6.11.2",
    "helmet": "^8.1.0",
    "serverless-http": "^3.2.0",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.152",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "eslint": "^9.32.0",
    "jest": "^30.0.5",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
